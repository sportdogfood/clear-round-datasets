{
  "version": "v2.5",
  "proxy_base": "https://crt-b1434e13de34.herokuapp.com",
  "git_base": "https://raw.githubusercontent.com/sportdogfood/clear-round-datasets/main",
  "fetch": { "order": ["proxy", "git"] },

  "paths": {
    "event_rules_path": "items/agents/event-rules.json",
    "venue_rules_path": "items/agents/venue-rules.json",
    "ingestion_rules_path": "items/agents/ingestion-rules.txt",
    "dir_map_path": "items/agents/dir-map.json"
  },

  "dirs": {
    "agents_dir": "items/agents",
    "events_dir": "index/events",
    "venues_dir": "index/venues",
    "organizers_dir": "index/organizers",
    "sources_dir": "index/sources"
  },

  "items": {
    "days_dir": "index/days",
    "months_dir": "index/months",
    "seasons_dir": "index/seasons",
    "years_dir": "index/years",
    "weeks_dir": "index/weeks",
    "countries_dir": "index/countries",
    "states_dir": "index/states",
    "cities_dir": "index/cities",
    "weather_dir": "index/weather",
    "airports_dir": "index/airports",
    "labels_dir": "index/labels",
    "forms_dir": "index/forms"
  },

  "aliases": { },

  "policy": {
    "uid_regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
    "filename_equals_uid": true,
    "uid_immutable": true,
    "date_format": "YYYY-MM-DD",
    "timezone_format": "IANA",
    "facts_only": true,
    "bool_encoding": "yn",
    "ignore_prefixes": ["dnu_", "ignore_"],
    "official_link_must_be_https": true,
    "source_uid_policy": "domain_slug",
    "source_type_policy": "first_path_token_or_key",
    "geo_input_modes": ["lat_long", "google_pin"],
    "lat_field": "venue_geo_lat",
    "long_field": "venue_geo_long",
    "pin_field": "venue_google_pin"
  }
}
